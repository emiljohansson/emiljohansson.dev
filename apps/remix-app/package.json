{
  "private": true,
  "name": "remix-app",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"PORT=3001 remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint": "eslint app"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "1.5.1",
    "@remix-run/vercel": "1.5.1",
    "@vercel/node": "1.15.2",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.5.1",
    "@remix-run/serve": "1.5.1",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "autoprefixer": "10.4.2",
    "concurrently": "7.0.0",
    "config": "workspace:*",
    "eslint": "8.15.0",
    "postcss": "8.4.6",
    "tailwindcss": "3.0.18",
    "tsconfig": "workspace:*",
    "typescript": "4.6.2"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  }
}
